  reg = <0x60000000 0x80000000>;
 };

 gpio-keys {
  compatible = "gpio-keys";
  #address-cells = <1>;
  #size-cells = <0>;
  autorepeat;

  button@0 {
   gpios = <&gpio0 4 1>;
   linux,code = <116>;
   label = "GPIO Key Power";
   linux,input-type = <1>;
   gpio-key,wakeup = <1>;
   debounce-interval = <100>;
  };
 };

 gpio-leds {
  compatible = "gpio-leds";

  green {
   label = "rock:green:user1";
   gpios = <&gpio0 12 1>;
   default-state = "off";
  };

  blue {
   label = "rock:blue:user2";
   gpios = <&gpio0 14 1>;
   default-state = "off";
  };

  sleep {
   label = "rock:red:power";
   gpios = <&gpio0 15 0>;
   default-state = "off";
  };
 };

