/ {
 model = "Google Chromecast";
 compatible = "google,chromecast", "marvell,berlin2cd", "marvell,berlin";

 chosen {
  bootargs = "console=ttyS0,115200 earlyprintk";
 };

 memory {
  device_type = "memory";
  reg = <0x00000000 0x20000000>;
 };

 leds {
  compatible = "gpio-leds";

  white {
   label = "white";
   gpios = <&portc 1 0>;
   default-state = "keep";
  };

  red {
   label = "red";
   gpios = <&portc 2 0>;
   default-state = "keep";
  };
 };
};





&sdhci0 {
 non-removable;
 status = "okay";
};

&uart0 { status = "okay"; };

&usb_phy1 { status = "okay"; };

&usb1 { status = "okay"; };
